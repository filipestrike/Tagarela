{
  "expo": {
    "name": "tagarela",
    "slug": "tagarela",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./src/common/assets/icons/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/common/assets/icons/Tagarela-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#5E5CB2"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/common/assets/icons/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.tagarela"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "expo": {
      "plugins": [
        "expo-image-picker",
        {
          "photosPermission": "custom photos permission",
          "cameraPermission": "Allow Tagarela to open the camera",
          "microphonePermission": true
        }
      ]
    }
  }
}
